{
    "name": "mez/queuejitsu-cli",
    "description": "QueueJitsu CLI",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Meredith",
            "email": "martin@sourceguru.net"
        }
    ],
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "stickee/*": "source",
            "mez/*": "source",
            "*": "dist"
        }
    },
    "require": {
        "php": "^7.1",
        "ext-pcntl": "*",
        "mez/queuejitsu": "^1 || ^2 || ^3.0.1",
        "roave/security-advisories": "dev-master",
        "symfony/console": "^3.3 || ^4.0",
        "zendframework/zend-config-aggregator": "^1.0",
        "zfcampus/zf-development-mode": "^3.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2",
        "jakub-onderka/php-parallel-lint": "^0.9.2 || ^1.0.0",
        "nikic/php-parser": "^3.1",
        "phpmd/phpmd": "^2.6",
        "phpro/grumphp": "^0.11.6 || ^0.15.0",
        "phpstan/phpstan": "^0.8.3 || ^0.9.0",
        "povils/phpmnd": "^1.1 || ^2.0",
        "predis/predis": "^1.1",
        "sebastian/phpcpd": "^3.0",
        "sensiolabs/security-checker": "^4.1 || ^6.0",
        "squizlabs/php_codesniffer": "^3.0",
        "vlucas/phpdotenv": "^2.4 || ^3.0",
        "zendframework/zend-servicemanager": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "QueueJitsu\\Cli\\": "src"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status"
    },
    "extra": {
        "zf": {
            "config-provider": "QueueJitsu\\Cli\\ConfigProvider"
        }
    },
    "bin": [
        "bin/qjitsu",
        "bin/qjitsu.php"
    ]
}
